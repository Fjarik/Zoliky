@model ZolikyWeb.Areas.App.Models.Main.DashboardModel

<div class="card-header">
	<div class="card-actions">
		<a class="" data-action="collapse">
			<i class="ti-minus"></i>
		</a>
		<a class="btn-minimize" data-action="expand">
			<i class="mdi mdi-arrow-expand"></i>
		</a>
		<a class="btn-close" data-action="close">
			<i class="ti-close"></i>
		</a>
	</div>
	<h4 class="card-title m-b-0">Žebříček podle XP - @Model.ClassName</h4>
</div>
<div class="card-body b-t collapse show">
	<table class="table v-middle no-border">
		<tbody id="xpClass">
			
		</tbody>
	</table>
</div>